train_net: "models/VGG16/coco/train_context.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
# stepsize: 160000
stepsize: 80000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 10000
# We still use the snapshot prefix, though
snapshot_prefix: "snapshots/coco_vgg16_1/"
#debug_info: true
